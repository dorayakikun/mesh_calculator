  },
  "homepage": "https://dorayakikun.github.io/waffle-map/",
  "devDependencies": {
    "@storybook/react": "^4.0.0-alpha.14",
    "@types/jest": "^23.3.1",
    "@types/react-test-renderer": "^16.0.1",
    "@types/storybook__addon-links": "^3.3.0",
    "@types/storybook__react": "^3.0.7",
    "awesome-typescript-loader": "^5.2.0",
    "connect": "^3.6.6",
    "core-js": "^2.5.7",
    "del": "^3.0.0",
    "docusaurus-init": "^1.0.1",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "json-loader": "^0.5.7",
    "prettier": "^1.14.0",
    "puppeteer": "^1.6.2",
    "react-test-renderer": "^16.4.2",
    "reg-keygen-git-hash-plugin": "^0.7.1",
    "reg-notify-github-plugin": "^0.7.2",
    "reg-notify-slack-plugin": "^0.7.5",
    "reg-publish-s3-plugin": "^0.7.10",
    "reg-suit": "^0.7.13",
    "regenerator-runtime": "^0.12.0",
    "serve-static": "^1.13.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.5.1",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.2.3",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
