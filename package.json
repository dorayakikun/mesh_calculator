    "regression": "build-storybook && npm run screenshot && reg-suit sync-expected && reg-suit compare && reg-suit publish",
    "screenshot": "node screenshot.js",
    "storybook": "start-storybook -p 6006 -c .storybook -s public",
    "test": "jest"
  },
