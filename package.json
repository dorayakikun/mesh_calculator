    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-leaflet": "^2.0.0",
    "react-redux": "^5.0.7",
    "redux": "^3.6.0",
    "redux-saga": "^0.16.0",
    "semantic-ui-react": "^0.82.1",
    "waffle-map-mesh-calculator-basic": "^2.2.0"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
